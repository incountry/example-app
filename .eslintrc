{
    "env": {
        "node": true,
        "es2020": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": "airbnb-base",
    "plugins": [
        "promise"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-multiple-empty-lines": 0,
        "max-len": 0
    }
}
